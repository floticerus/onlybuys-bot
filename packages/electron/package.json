{
  "name": "@onlybuys-bot/electron",
  "author": "Kevin <floticerus@users.noreply.github.com>",
  "version": "0.0.35",
  "description": "UI application for onlybuys-bot",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "release:windows": "electron-forge publish --platform windows",
    "release:linux": "electron-forge publish --platform linux",
    "release:mac": "electron-forge publish --platform darwin",
    "lint": "eslint . --ext js,ts",
    "test": "jest"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "styled-components": "5.3.0",
    "tsutils": "~3.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@onlybuys-bot/cli": "file:../cli",
    "@radix-ui/react-alert-dialog": "^0.1.7",
    "@radix-ui/react-checkbox": "^0.1.5",
    "@radix-ui/react-dialog": "^0.1.7",
    "@radix-ui/react-dropdown-menu": "^0.1.6",
    "@radix-ui/react-hover-card": "^0.1.5",
    "@radix-ui/react-label": "^0.1.5",
    "@radix-ui/react-separator": "^0.1.4",
    "@radix-ui/react-slider": "^0.1.4",
    "@radix-ui/react-switch": "^0.1.5",
    "@radix-ui/react-toast": "^0.1.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@timfish/forge-externals-plugin": "^0.1.2",
    "@types/": "fortawesome/react-fontawesome",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "5.1.10",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "@typescript-eslint/utils": "5.14.0",
    "abort-controller": "^3.0.0",
    "autolinker": "^3.15.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "commander": "^9.0.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.0",
    "dotenv": "^16.0.0",
    "electron": "13.1.2",
    "electron-store": "8.0.1",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-utils": "3.0.0",
    "ethers": "^5.5.4",
    "file-loader": "^6.2.0",
    "framer-motion": "^6.2.8",
    "jest": "27.0.4",
    "node-fetch": "^3.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "prettier": "2.3.1",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "react-use": "^17.3.2",
    "style-loader": "^3.3.1",
    "tailwind-styled-components": "^2.1.4",
    "tailwindcss": "^3.0.23",
    "ts-jest": "27.0.3",
    "typescript": "4.3.4",
    "wait-on": "5.3.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "BleepBloop",
        "executableName": "bleep-bloop",
        "icon": "assets/icon",
        "extraResource": [
          "assets"
        ],
        "afterCopy": [
          "./build-scripts/copy-cli.js"
        ],
        "prune": false
      },
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/main.webpack.js",
            "renderer": {
              "config": "./webpack/renderer.webpack.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./electron/bridge.ts"
                  }
                }
              ]
            }
          }
        ]
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "BleepBloop"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "floticerus",
              "name": "onlybuys-bot"
            },
            "prerelease": true
          }
        }
      ]
    }
  }
}
