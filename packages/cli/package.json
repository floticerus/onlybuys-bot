{
  "name": "@onlybuys-bot/cli",
  "version": "0.0.36",
  "description": "EVM bot with new pair watcher and trading",
  "engines": {
    "node": ">= 16.13"
  },
  "main": "build/src/main.js",
  "devDependencies": {
    "@dopex-io/web3-multicall": "^0.1.6",
    "@ethersproject/shims": "^5.6.0",
    "@types/jest": "~27.0.2",
    "@typescript-eslint/eslint-plugin": "~5.14.0",
    "@typescript-eslint/parser": "~5.14.0",
    "boolean": "^3.2.0",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.0",
    "eslint": "~8.4.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~25.3.0",
    "ethers": "^5.5.4",
    "forever": "^4.0.3",
    "get-random-values": "^1.2.2",
    "jest": "~27.4.4",
    "prettier": "~2.5.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.1.1",
    "ts-loader": "^9.2.8",
    "tsutils": "~3.21.0",
    "typescript": "4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "start:forever": "forever build/src/main.js",
    "start:all": "node build/src/master.js",
    "start:all:forever": "forever build/src/master.js",
    "start:eth": "yarn start -- --network=eth",
    "start:bsc": "yarn start -- --network=bsc",
    "start:polygon": "yarn start -- --network=polygon",
    "start:metis": "yarn start -- --network=metis",
    "start:fuse": "yarn start -- --network=fuse",
    "start:avalanche": "yarn start -- --network=avalanche",
    "start:fantom": "yarn start -- --network=fantom",
    "start:moonriver": "yarn start -- --network=moonriver",
    "start:moonbeam": "yarn start -- --network=moonbeam",
    "bundle": "yarn webpack",
    "maybe-sell-all": "node build/src/maybe-sell-all.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "yarn lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "portfolio": "node build/src/output-portfolio.js",
    "privatekey": "node build/src/output-private-key.js",
    "help": "node build/src/main.js --help"
  },
  "author": "Kevin <floticerus@users.noreply.github.com>",
  "license": "Apache-2.0"
}
