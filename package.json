{
  "name": "onlybuys-bot",
  "version": "0.0.1",
  "description": "A bot that only buys.",
  "engines": {
    "node": ">= 16.13"
  },
  "devDependencies": {
    "@types/jest": "~27.0.2",
    "@types/node": "~17.0.21",
    "@typescript-eslint/eslint-plugin": "~5.7.0",
    "@typescript-eslint/parser": "~5.7.0",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "dotenv": "^16.0.0",
    "eslint": "~8.4.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~25.3.0",
    "ethers": "^5.5.4",
    "forever": "^4.0.3",
    "jest": "~27.4.4",
    "prettier": "~2.5.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.1.1",
    "tsutils": "~3.21.0",
    "typescript": "4.5.5"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "start:forever": "forever build/src/main.js",
    "start:eth": "npm run start -- --network=eth",
    "start:bsc": "npm run start -- --network=bsc",
    "start:polygon": "npm run start -- --network=polygon",
    "start:metis": "npm run start -- --network=metis",
    "start:fuse": "npm run start -- --network=fuse",
    "start:avalanche": "npm run start -- --network=avalanche",
    "start:fantom": "npm run start -- --network=fantom",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "portfolio": "node build/src/output-portfolio.js",
    "privatekey": "node build/src/output-private-key.js",
    "help": "node build/src/main.js --help"
  },
  "author": "Kevin <floticerus@users.noreply.github.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "tslib": "~2.3.1"
  }
}
